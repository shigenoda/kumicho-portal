# App Configuration
VITE_APP_ID=your-app-id

# Security
JWT_SECRET=your-jwt-secret-key-here

# Database (PostgreSQL)
# For Neon: postgres://user:password@ep-xxxxx.us-east-1.aws.neon.tech/dbname?sslmode=require
# For Vercel Postgres: postgres://user:password@region.postgres.vercel-storage.com:5432/dbname
# For local development: postgres://user:password@localhost:5432/database
DATABASE_URL=postgres://user:password@host:port/database

# OAuth (Optional - remove if not using OAuth)
OAUTH_SERVER_URL=https://your-oauth-server.com
OWNER_OPEN_ID=owner-open-id

# AI/LLM API (Optional - remove if not using)
BUILT_IN_FORGE_API_URL=https://api.example.com
BUILT_IN_FORGE_API_KEY=your-api-key-here

# Environment
NODE_ENV=production
